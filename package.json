{
  "name": "blogcraft",
  "version": "1.0.0",
  "description": "Um editor avançado para o Blogger com recursos moderno e integração completa",
  "private": true,
  "scripts": {
  "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
  "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
  "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts test",
  "eject": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts eject"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "@tinymce/tinymce-react": "^4.3.0",
    "file-saver": "^2.0.5",
    "react": "^18.2.0",
    "react-datetime-picker": "^5.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
