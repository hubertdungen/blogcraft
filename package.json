{
  "name": "blogcraft",
  "version": "1.0.0",
  "description": "Um editor avançado para o Blogger com recursos modernos e integração completa",
  "private": true,
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired build",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && react-app-rewired test",
    "eject": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts eject",
    "scheduler": "node scheduler.js"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^35.4.0",
    "@ckeditor/ckeditor5-react": "^5.0.6",
    "@ckeditor/ckeditor5-theme-lark": "^46.0.1",
    "@react-oauth/google": "^0.12.1",
    "file-saver": "^2.0.5",
    "node-schedule": "^2.1.1",
    "react": "^18.2.0",
    "react-datetime-picker": "^5.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@ckeditor/ckeditor5-dev-utils": "^43.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "postcss-import": "^16.1.1",
    "react-app-rewired": "^2.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
